<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="profexp">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="exp" maxOccurs="unbounded" minOccurs="0">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="org" type="xs:string" />
              <xs:element name="title" type="xs:string" />
              <xs:element name="duties">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="duty" type="xs:string> maxOccurs="unbounded" minOccurs="0" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:scema>

